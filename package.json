  "private": true,
  "dependencies": {
    "@chiragrupani/fullscreen-react": "^1.1.1",
    "@devexpress/dx-react-grid": "^2.7.5",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "devextreme": "^20.2.4",
    "devextreme-react": "20.2.3",
    "dompurify": "^2.3.0",
    "file-saver": "^2.0.5",
    "iframe-resizer-react": "^1.1.0",
    "js-file-download": "^0.4.12",
    "jspdf": "^2.3.1",
    "jspdf-autotable": "^3.5.14",
    "jwt-decode": "^3.1.2",
    "node-sass": "^4.11.0",
    "react": "^16.2.0",
    "react-app-polyfill": "^1.0.0",
    "react-dom": "^16.2.0",
    "react-html2pdf": "^1.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "4.0.3",
    "react-split-pane": "^0.1.92",
    "react-tabs": "^3.2.2",
    "react-tiny-fab": "^4.0.3",
    "react-zoom-pan-pinch": "^2.1.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start-dev": "set PORT=3000 && env-cmd -e dev react-scripts start",
    "start-test": "set PORT=3000 && env-cmd -e test react-scripts start",
    "build-dev": "env-cmd -e dev react-scripts --expose-gc --max-old-space-size=4048 build",
    "build-test": "env-cmd -e test react-scripts --expose-gc --max-old-space-size=4048 build",
    "start-prod": "env-cmd -e prod react-scripts start",
    "build-prod": "env-cmd -e prod react-scripts --expose-gc --max-old-space-size=4048 build",
    "eject": "react-scripts eject",
    "build-themes": "devextreme build",
    "postinstall": "npm run build-themes"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "devextreme-cli": "1.2.14",
    "devextreme-themebuilder": "^20.2.4"
  }
}
